{
	"name": "@ctx-core/cli-args",
	"version": "11.4.18",
	"description": "ctx-core command line interface (cli) args library",
	"keywords": [
		"ctx-core",
		"args"
	],
	"homepage": "https://github.com/ctx-core/cli-args#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/cli-args/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/cli-args.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./arg_a__pick",
		"./default_arg_a_",
		"./dfn_flag_r_",
		"./flag_a_",
		"./flag_r_",
		"./flag_r__pick",
		"./flag_R_arg_a_",
		"./flag_R_param_name_",
		"./flag_regex",
		"./param_dfn_a_reducer",
		"./param_dfn_split_regex",
		"./param_name_R_param_val_a_",
		"./param_r_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.18",
		"@ctx-core/function": "^21.22.13",
		"@ctx-core/object": "^31.0.1"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}