{
	"name": "@ctx-core/cli-args",
	"version": "11.0.2",
	"description": "ctx-core command line interface (cli) args library",
	"keywords": [
		"ctx-core",
		"args"
	],
	"homepage": "https://github.com/ctx-core/cli-args#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/cli-args/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/cli-args.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.25",
		"@ctx-core/function": "workspace:^20.2.5",
		"@ctx-core/object": "workspace:^21.0.24"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.110",
		"typescript": "^4.5.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}